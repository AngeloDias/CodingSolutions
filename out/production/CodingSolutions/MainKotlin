import firstChapter.ArrayList

fun main() {
    val birdsList = ArrayList()
    val getPosition = 3
    val removePosition = 7

    print("- Is arrayList empty?\n- Answer: ${birdsList.isEmpty()}")

    birdsList.add(22)
    birdsList.add(14)
    birdsList.add(7)
    birdsList.add(52)
    birdsList.add(82)

    print("- Is arrayList empty?\n- Answer: ${birdsList.isEmpty()}")

    birdsList.iterator()

    print("Get element at position $getPosition: ${birdsList.get(getPosition)}")
    print("Remove element at position $removePosition: ${birdsList.remove(removePosition)}")

    birdsList.iterator()
}
